	"documentnotes": {
		"note": [
			{
				"text": "\n            A cleartext storage of sensitive information vulnerability in FortiOS command line interface may allow an authenticated attacker to obtain sensitive information such as users passwords by connecting to FortiGate CLI and executing the \"diag sys ha checksum show\" command.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Information disclosure\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            FortiOS versions 6.2.4 and below.FortiOS versions 6.0.11 and below.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Please upgrade to FortiOS version 6.4.0 or above.Please upgrade to FortiOS version 6.2.5 or above.Please upgrade to FortiOS version 6.0.12 or above.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
