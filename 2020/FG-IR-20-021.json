	"documentnotes": {
		"note": [
			{
				"text": "\n            An unquoted service path vulnerability in the FortiSIEMWindowsAgent component may allow an attacker to gain elevated privileges via the AoWinAgt executable service path.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Escalation of privilege\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            FortiSIEMWindowsAgent version 3.1.2 and below.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Please upgrade to FortiSIEMWindowsAgent version 3.2.0 or above.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
