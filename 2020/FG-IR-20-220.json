	"documentnotes": {
		"note": [
			{
				"text": "\n            A use of password hash with insufficient computational effort vulnerability [CWE-916] in FortiSandbox may allow an attacker with access to the password database to efficiently mount bulk guessing attacks to recover the passwords.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            None\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Execute unauthorized code or commands\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            FortiDeceptor 24 all versions are not affectedFortiDeceptor 6.0 all versions are not affectedFortiDeceptor 5.3 all versions are not affectedFortiDeceptor 5.2 all versions are not affectedFortiDeceptor 5.1 all versions are not affectedFortiDeceptor version 5.0.0FortiDeceptor 4.3 all versionsFortiDeceptor 4.2 all versionsFortiDeceptor 4.1 all versionsFortiDeceptor 4.0 all versionsFortiDeceptor 3.3 all versionsFortiDeceptor 3.2 all versionsFortiDeceptor 3.1 all versionsFortiDeceptor 3.0 all versionsFortiSandbox 5.0 all versions are not affectedFortiSandbox 4.4 all versions are not affectedFortiSandbox 4.2 all versions are not affectedFortiSandbox 4.0 all versionsFortiSandbox 3.2 all versions\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "4"
			},
			{
				"text": "\n            Upgrade to FortiSandbox version 4.2.0 and above.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "5"
			}
		]
	},
