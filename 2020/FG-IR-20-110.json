	"documentnotes": {
		"note": [
			{
				"text": "\n            An execution with unnecessary privileges vulnerability in the VCM engine of FortiClient for Linux may allow local users to elevate their privileges to root by creating a malicious script or program on the target machine.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            None\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Privilege Escalation\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            FortiClient for Linux versions 6.2.7 and below. FortiClient for Linux versions 6.4.0 and below.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "4"
			},
			{
				"text": "\n            Please upgrade to FortiClient for Linux versions 6.2.8 or above. Please upgrade to FortiClient for Linux versions 6.4.1 or above.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "5"
			}
		]
	},
