	"documentnotes": {
		"note": [
			{
				"text": "\n            An improper neutralization of special elements used in an OS Command vulnerability in FortiSandbox may allow an authenticated attacker with access to the web GUI to execute unauthorized code or commands via specifically crafted HTTP requests.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Execute unauthorized code or commands\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            FortiSandbox 3.2.2 and below.FortiSandbox 3.1.4 and below.FortiSandbox 3.0.6 and below.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Upgrade to FortiSandbox 4.0.0 or above.Upgrade to FortiSandbox 3.2.3 or above.Upgrade to FortiSandbox 3.1.5 or above.Upgrade to FortiSandbox 3.0.7 or above.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
