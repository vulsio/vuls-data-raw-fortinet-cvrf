	"documentnotes": {
		"note": [
			{
				"text": "\n            An improper authentication vulnerability [CWE-287] in FSSO Collector may allow an unauthenticated user to bypass any firewall authentication rule and access the protectedÂ network via sending specifically crafted UDP login notification packets.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Unauthorized Access\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Any FSSO DC Agent and Collector released with FOS 7.0.0 or below is impacted. Any FSSO DC Agent and Collector released with FOS 6.4.5 or below is impacted.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Upgrade the FSSO DC Agent and Collector with any version released with FOS 7.0.1Â or above. Upgrade the FSSO DC Agent and Collector with any version released with FOS 6.4.6 or above.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
