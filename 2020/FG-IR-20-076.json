	"documentnotes": {
		"note": [
			{
				"text": "\n            An information disclosure vulnerability in FortiWeb's Web Vulnerability Scan profile may allow a remote authenticated attacker to read the password used by the FortiWeb scanner to access the device defined in the scan profile.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            None\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Information disclosure\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            FortiWeb version 6.2.3 and below.FortiWeb version 6.3.4 and below.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "4"
			},
			{
				"text": "\n            Please upgrade to version 6.2.4 or above.Please upgrade to version 6.3.5 or above.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "5"
			}
		]
	},
