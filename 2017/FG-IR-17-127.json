	"documentnotes": {
		"note": [
			{
				"text": "\n            Two XSS vulnerabilities were reported to us affecting FortiOS that can be exploited to load and run a remote (malicious) Javascript in a logged in browser.The vulnerable input fields are the \"Comments\" input while saving Configuration Revisions (CVE-2017-7734) the \"Groups\" input while creating or editing User Groups (CVE-2017-7735)\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Execute unauthorized code or commands\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            CVE-2017-7734: FortiOS versions 5.4.0 to 5.4.4CVE-2017-7735: FortiOS versions 5.2.0 to 5.4.4\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Upgrade to FortiOS 5.4.5 or 5.6.0\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
