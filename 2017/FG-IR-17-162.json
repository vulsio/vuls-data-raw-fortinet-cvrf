	"documentnotes": {
		"note": [
			{
				"text": "\n            The HTML source code of the FortiWeb SNMPv3 user edit webui page includes the user's password in cleartext.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Information disclosure\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            FortiWeb 5.8.2 and below until 5.4.1.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Upgrade to FortiWeb version 5.8.3\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
