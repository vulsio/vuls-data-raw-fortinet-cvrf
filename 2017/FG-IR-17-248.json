	"documentnotes": {
		"note": [
			{
				"text": "\n            FortiWebManager 5.8.0 fails to check the admin password, granting access regardless the provided string.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Improper Access Control\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Only FortiWebManager 5.8.0 is affected.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Users on FortiWebManager 5.8.0 must upgrade to 5.8.1.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
