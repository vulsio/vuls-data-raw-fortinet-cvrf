	"documentnotes": {
		"note": [
			{
				"text": "\n            An old Infineon RSA library does not properly generate RSA key pairs, therefore enabling an attacker to potentially infer a private key from a public key.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Breaking RSA encryption\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            The following Fortinet products are NOT affected:FortiOSFortiSwitchFortiAPFortiAnalyzerFortiMailfortiManagerFortiWebFortiTokenFortiAuthenticatorNone of the products above are using the affected Infineon RSA library or TPM chips.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			}
		]
	},
