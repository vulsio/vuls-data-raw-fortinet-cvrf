	"documentnotes": {
		"note": [
			{
				"text": "\n            The FortiAnalyzer and FortiManager WebUI accept a user-controlled input that specifies a link to an external site, and uses that link in a redirect.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Execute unauthorized code or commands\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            FortiAnalyzer versions 5.4.0 to 5.4.2,FortiManager versions 5.4.0 to 5.4.2.Preceding versions of FortiAnalyzer and FortiManager are not impacted\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            For FortiAnalyzer: Upgrade to version 5.4.3 or 5.6.0 For FortiManager: Upgrade to version 5.4.3 or 5.6.0\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
