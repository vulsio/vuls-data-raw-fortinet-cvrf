	"documentnotes": {
		"note": [
			{
				"text": "\n            The FortiWLC file management AP script download webUI page is affected by an OS Command Injection vulnerability which may allow an authenticated admin user to execute arbitrary system console commands, and possibly subsequently \"root\" the device.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Execute unauthorized code or commands\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            FortiWLC 6.1-2 -> 6.1-5FortiWLC 7.0-7 -> 7.0-10FortiWLC 8.0 -> 8.2FortiWLC 8.3.0 -> 8.3.2\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            For FortiWLC 7.x branch, upgrade to 7.0.11 or newer versions. For FortiWLC 8.x branch, upgrade to 8.3.3 or newer versions.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
