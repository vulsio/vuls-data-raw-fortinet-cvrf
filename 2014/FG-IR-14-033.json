		"note": [
			{
				"text": "\n            Prior to version 5.0.7, the Web User Interface of FortiManager and FortiAnalyzer is vulnerable to multiple reflected Cross-Site Scripting vulnerabilities.\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "1"
			},
			{
				"text": "\n            Cross-Site Scripting (reflected)\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            FortiManager and FortiAnalyzer < version 5.0.7\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Upgrade to 5.0.7 or above.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
