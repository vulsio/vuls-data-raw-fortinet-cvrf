	"documentnotes": {
		"note": [
			{
				"text": "\n            Multiple CSRF vulnerabilities exist in the FortiWeb web administration console due to lack of CSRF token protection. This could allow remote attackers to perform administrative actions under specific conditions.\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "1"
			},
			{
				"text": "\n            Authorization Bypass\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            FortiWeb 5.1.x and lower.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Upgrade to FortiWeb 5.2.0 or higher.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
