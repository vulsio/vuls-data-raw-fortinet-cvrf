	"documentnotes": {
		"note": [
			{
				"text": "\n            FortiWeb 5.0, 5.1 and 5.2.0 are vulnerable to multiple reflective cross-site scripting issues. Several parameters in the web management interface URLs /user/ldap_user/check_dlg and /user/radius_user/check_dlg lack sufficient input filtering.\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "1"
			},
			{
				"text": "\n            Cross-Site Scripting\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            FortiWeb 5.0.x, 5.1.x and 5.2.0.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Upgrade to FortiWeb 5.2.1 or higher.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
