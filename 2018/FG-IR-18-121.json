	"documentnotes": {
		"note": [
			{
				"text": "\n            An attacker could send DHCP request containing malicious scripts in the HOSTNAME parameter. The malicious script code is executed while viewing the logs in FortiAnalyzer and FortiManager (with FortiAnalyzer feature enabled).\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Cross-site Scripting (XSS)\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            FortiAnalyzer 5.6.0 and below versions. FortiManager 5.6.0 and below versions.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Upgrade to FortiAnalyzer 5.6.1 or above.Upgrade to FortiManager 5.6.1 or above.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
