	"documentnotes": {
		"note": [
			{
				"text": "\n            ntp released an announcement on 26th April 2016, describing 4 low and 7 medium severity vulnerabilities, as listed below:- CVE-2016-1551- CVE-2016-1549- CVE-2016-2516- CVE-2016-2517- CVE-2016-2518- CVE-2016-2519- CVE-2016-1547- CVE-2016-1548- CVE-2015-7704- CVE-2015-8138- CVE-2015-1550\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Information disclosure\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            FortiOS versions 5.4.1, 5.4.0, 5.2.8 and below are vulnerable to only CVE-2016-1550.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Upgrade to FortiOS version5.2.9 or above5.4.2 or aboveor 5.6.0\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
