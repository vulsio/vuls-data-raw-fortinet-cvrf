	"documentnotes": {
		"note": [
			{
				"text": "\n            FortiWLC comes with a hardcoded account named 'core' which is used by Meru Access Points to send core dumps to the FortiWLC and has read/write privileges over various parts of the system.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Unauthorized read/write remote access\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            FortiWLC 7.0-9-1, 7.0-10-0, 8.1-2-0, 8.1-3-2 and 8.2-4-0\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Depending on your version, apply the following patches:7.0-9-1:meru-7.0-9-1-patch-bug03932927.0-10-0:meru-7.0-10-0-patch-bug03932928.1-2-0:meru-8.1-2-0-patch-bug03932928.1-3-2:meru-8.1-2-0-patch-bug03932928.2-4-0:meru-8.2-4-0-patch-bug0393292\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
