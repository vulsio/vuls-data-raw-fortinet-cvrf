	"documentnotes": {
		"note": [
			{
				"text": "\n            A race condition in the tty_ioctl function in drivers/tty/tty_io.c in the Linux kernel may allow local users to obtain sensitive information from kernel memory or cause a denial of service.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Denial of service\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            FortiOS 5.4.0 to 5.4.3 FortiOS 5.4.0 to 5.4.3 FortiOS 5.4.0 to 5.4.3 FortiOS 5.4.0 to 5.4.3 FortiOS 5.4.0 to 5.4.3 FortiOS 5.4.0 to 5.4.3 FortiOS 5.4.0 to 5.4.3\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Upgrade to FortiOS version Upgrade to FortiOS version Upgrade to FortiOS version Upgrade to FortiOS version Upgrade to FortiOS version Upgrade to FortiOS version Upgrade to FortiOS version 5.4.4 or 5.6.0 5.4.4 or 5.6.0 5.4.4 or 5.6.0 5.4.4 or 5.6.0 5.4.4 or 5.6.0 5.4.4 or 5.6.0 5.4.4 or 5.6.0\n        ",
