	"documentnotes": {
		"note": [
			{
				"text": "\n            FortiManager does not properly validate TLS certificates when probing for devices to administer. This leads to potential pre-shared secret exposure.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            FortiManager does not properly validate TLS certificates when probing for devices to administer. This leads to potential pre-shared secret exposure.\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Credentials exposure\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            FortiManager 5.0.6 to 5.2.7 and 5.4.0 to 5.4.1.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "4"
			},
			{
				"text": "\n            Upgrade to FMG 5.2.8 and 5.4.2\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "5"
			}
		]
	},
