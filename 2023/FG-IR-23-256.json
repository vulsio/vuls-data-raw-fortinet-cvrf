	"documentnotes": {
		"note": [
			{
				"text": "\n            An improper output neutralization for logs vulnerability [CWE-117] in FortiWeb Traffic Log component may allow an attacker to forge traffic logs via a crafted URL of the web application.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            None\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Information disclosure\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            FortiWeb version 7.4.0 FortiWeb version 7.2.0 through 7.2.5 FortiWeb 7.0 all versions FortiWeb 6.3 all versions FortiWeb 6.2 all versions\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "4"
			},
			{
				"text": "\n            Please upgrade to FortiWeb version 7.4.1 or above Please upgrade to FortiWeb version 7.2.6 or above\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "5"
			}
		]
	},
