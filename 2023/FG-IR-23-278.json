	"documentnotes": {
		"note": [
			{
				"text": "\n            A Cleartext Storage of Sensitive Information vulnerability [CWE-312] in FortiClient Windows may permit a local authenticated user to retrieve VPN password via memory dump, due to JavaScript's garbage collector\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            None\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Information disclosure\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            None\n        ",
