	"documentnotes": {
		"note": [
			{
				"text": "\n            A NULL pointer dereference vulnerability [CWE-476] in SSL-VPN may allow an authenticated remote attacker to trigger a crash of the SSL-VPN service via crafted requests.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            None\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Denial of service\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            FortiOS version 7.2.0 through 7.2.4FortiOS version 7.0.0 through 7.0.10FortiOS version 6.4.0 through 6.4.12FortiProxy version 7.2.0 through 7.2.3FortiProxy version 7.0.0 through 7.0.9\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "4"
			},
			{
				"text": "\n            Please upgrade to FortiOS version 7.4.0 or abovePlease upgrade to FortiOS version 7.2.5 or abovePlease upgrade to FortiOS version 7.0.11 or abovePlease upgrade to FortiOS version 6.4.13 or abovePlease upgrade to FortiProxy version 7.2.4 or abovePlease upgrade to FortiProxy version 7.0.10 or above\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "5"
			}
		]
	},
