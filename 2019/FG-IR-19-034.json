	"documentnotes": {
		"note": [
			{
				"text": "\n            Failure to sanitize input in the SSL VPN web portal may allow an attacker to perform a reflected Cross-site Scripting (XSS) attack via multiple parameters of the error page HTTP request.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Cross-site Scripting (XSS)\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            CVE-2019-5586FortiOS 6.0.0 to 6.0.4FortiOS 5.2.0 to 5.6.10CVE-2019-5588FortiOS 6.0.0 to 6.0.4\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Upgrade to FortiOS 5.6.11, 6.0.5 or 6.2.0 Workarounds: Disable the SSL-VPN web portal service by applying the following CLI commands: config vpn ssl settings unset source-interface end Revision History: 05-24-2019 Initial version 08-21-2019 Add 5.6 branch fixing for CVE-2019-5586\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
