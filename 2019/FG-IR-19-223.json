	"documentnotes": {
		"note": [
			{
				"text": "\n            An improper neutralization of input during web page generation in the SSL VPN portal of FortiOS may allow a remote authenticated attacker to perform a stored cross site scripting attack (XSS).\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Execute unauthorized code or commands\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            FortiOS version 6.2.1 and below. FortiOS version 6.0.8 and below. FortiOS version 5.6.12 and below.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Please upgrade to FortiOS version 6.2.2 or above. Please upgrade to FortiOS version 6.0.9 or above. Please upgrade to FortiOS version 5.6.13 or above.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
