	"documentnotes": {
		"note": [
			{
				"text": "\n            An improper input validation (CWE-20) vulnerability in FortiAP CLI admin console may allow unauthorized administrators to overwrite system files via specially crafted tcpdump commands in the CLI.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Improper Input Validation\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            FortiAP-S/W2 6.2.0 to 6.2.2, 6.0.5 and belowFortiAP-U 6.0.1 and belowFortiAP is not impactedFortiAP-C is not impacted\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Upgrade to FortiAP-S/W2 6.0.6 or 6.2.3 and above Upgrade to FortiAP-U 6.0.2 or above Revision History: 02-10-2020 Initial version 05-25-2020 Added FAP, FAP-U and FAP-C impact info.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
