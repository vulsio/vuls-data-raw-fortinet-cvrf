	"documentnotes": {
		"note": [
			{
				"text": "\n            Improper implementations of the HTTP/2 protocol can lead to a variety denial-of-service (DoS) attacks.The related CVEs are:CVE-2019-9511, also known as Data DribbleCVE-2019-9512, also known as Ping FloodCVE-2019-9513, also known as Resource LoopCVE-2019-9514, also known as Reset FloodCVE-2019-9515, also known as Settings FloodCVE-2019-9516, also known as 0-Length Headers LeakCVE-2019-9517, also known as Internal Data BufferingCVE-2019-9518, also known as Empty Frame Flooding\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Denial of service\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            The following products have been confirmed to NOT be vulnerable to any of the above:FortiOSFortiAPFortiSwitchFortiAnalyzerFortiWebFortiManagerFortiMail\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			}
		]
	},
