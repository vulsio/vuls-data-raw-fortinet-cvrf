	"documentnotes": {
		"note": [
			{
				"text": "\n            An improper neutralization of input vulnerability in the FortiADC may allow an attacker to execute a stored Cross Site Scripting (XSS) via a field in the traffic group interface.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Execute unauthorized code or commands\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            FortiADC version 5.3.3 and below\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Please upgrade to FortiADC version 5.3.4 or above\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
