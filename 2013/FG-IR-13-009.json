	"documentnotes": {
		"note": [
			{
				"text": "\n            FortiWeb 5.0.2 and lower are vulnerable to cross-site scripting (CVE-2014-1955), HTTP header injection (CVE-2014-1956) and privilege escalation (CVE-2014-1957) issues.\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "1"
			},
			{
				"text": "\n            Script execution and privilege elevation.\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            FortiWeb 4.4.7 and lower.FortiWeb 5.0.2 and lower.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Upgrade to FortiWeb 5.0.3 or higher.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
