	"documentnotes": {
		"note": [
			{
				"text": "\n            A use of hard-coded cryptographic key vulnerability [CWE-321] in FortiDDoS API may allow an attacker who managed to retrieve the key from one device to sign JWT tokens for any device.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            None\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Improper access control\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            FortiDDoS 5.7 all versions are not affectedFortiDDoS 5.6 all versions are not affectedFortiDDoS 5.5 all versionsFortiDDoS version 5.4.0 through 5.4.2FortiDDoS version 5.3.0 through 5.3.1FortiDDoS 5.2 all versionsFortiDDoS 5.1 all versions\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "4"
			},
			{
				"text": "\n            Please upgrade to upcoming FortoDDoS 5.6.0 or above.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "5"
			}
		]
	},
