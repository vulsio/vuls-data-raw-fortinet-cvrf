	"documentnotes": {
		"note": [
			{
				"text": "\n            An improper neutralization of special elements used in a template engine vulnerability [CWE-1336] in FortiSOAR management interface may allow a remote and authenticated attacker to execute arbitrary code via a crafted payload.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            None\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Execute unauthorized code or commands\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            FortiSOAR 7.6 all versions are not affectedFortiSOAR 7.5 all versions are not affectedFortiSOAR 7.4 all versions are not affectedFortiSOAR 7.3 all versions are not affectedFortiSOAR version 7.2.0FortiSOAR 7.0 all versionsFortiSOAR 6.4 all versions\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "4"
			},
			{
				"text": "\n            Please upgrade to FortiSOAR version 7.2.1 or above\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "5"
			}
		]
	},
