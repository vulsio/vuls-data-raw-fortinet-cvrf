	"documentnotes": {
		"note": [
			{
				"text": "\n            An improper control of a resource through its lifetime vulnerability [CWE-664] in FortiEDR CollectorWindows may allow a privileged user to terminate the FortiEDR processes with special tools and bypass the EDR protection.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            None\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Execute unauthorized code or commands\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            FortiEDR CollectorWindows version 5.1.0FortiEDR CollectorWindows version 5.0.0 through 5.0.3.751FortiEDR CollectorWindows version 4.0.0 through 4.1\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "4"
			},
			{
				"text": "\n            Please upgrade to FortiEDR CollectorWindows version 5.0.3.912 or abovePlease upgrade to FortiEDR CollectorWindows version 5.2.0.2288 or above\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "5"
			}
		]
	},
