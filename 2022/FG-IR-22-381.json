	"documentnotes": {
		"note": [
			{
				"text": "\n            A permissive list of allowed inputs vulnerability [CWE-183] in FortiGate Policy-based NGFW Mode may allow an authenticated SSL-VPN user to bypass the policy via bookmarks in the web portal.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            None\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Improper access control\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            FortiOS version 7.2.0 through 7.2.3 FortiOS version 7.0.0 through 7.0.9\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "4"
			},
			{
				"text": "\n            Please upgrade to FortiOS version 7.2.4 or above Please upgrade to FortiOS version 7.0.11 or above\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "5"
			}
		]
	},
