	"documentnotes": {
		"note": [
			{
				"text": "\n            An improper neutralization of special elements used in an OS command vulnerability [CWE-78]Â in the management interface of FortiWAN may allow an authenticated attacker to execute unauthorized commands via specifically crafted arguments to existing commands.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            None\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Execute unauthorized code or commands\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            FortiWAN version 4.5.0 through 4.5.9FortiWAN version 4.4.0 through 4.4.1FortiWAN version 4.3.0 through 4.3.1FortiWAN version 4.2.5 through 4.2.7FortiWAN version 4.2.1 through 4.2.2FortiWAN version 4.1.1 through 4.1.3FortiWAN version 4.0.0 through 4.0.6\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "4"
			},
			{
				"text": "\n            Please upgrade to FortiWAN version 4.5.10 or above\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "5"
			}
		]
	},
