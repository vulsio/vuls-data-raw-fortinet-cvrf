	"documentnotes": {
		"note": [
			{
				"text": "\n            An improper neutralization of special elements used in an SQL command ('SQL Injection') vulnerability [CWE-89] in FortiWLM may allow an authenticated attacker to alter the query logic and execute arbitrary SQL statements via crafted HTTP requests to the AP monitor handlers.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            None\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Execute unauthorized code or commands\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            FortiWLM version 8.6.2 and below. FortiWLM version 8.5.2 and below. FortiWLM version 8.4.2 and below. FortiWLM version 8.3.2 and below.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "4"
			},
			{
				"text": "\n            Upgrade to FortiWLM version 8.6.3 or above.  \n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "5"
			}
		]
	},
