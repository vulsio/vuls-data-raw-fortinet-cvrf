	"documentnotes": {
		"note": [
			{
				"text": "\n            An improper validation of certificate with host mismatch vulnerability [CWE-297] in FortiTokenMobile may allow an unauthenticated user to spoof the validation server identity and achieve a Man-in-the-Middle attack.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            None\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Improper server authentication\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            FortiTokenAndroid 5.0 all versionsFortiTokenAndroid 4.5 all versionsFortiTokenAndroid 4.4 all versionsFortiTokenAndroid 4.3 all versionsFortiTokenAndroid 4.2 all versionsFortiTokenAndroid 4.1 all versionsFortiTokenAndroid 4.0 all versionsFortiTokenAndroid 3.0 all versionsFortiTokenAndroid 0.4 all versionsFortiTokenIOS 5.2 all versionsFortiTokenIOS 4.3 all versionsFortiTokenIOS 4.2 all versionsFortiTokenIOS 4.1 all versionsFortiTokenIOS 3.0 all versionsFortiTokenMobileWP 4.0 all versionsFortiTokenMobileWP 3.0 all versions\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "4"
			},
			{
				"text": "\n            Upgrade FortiTokenMobile for Android to version 5.1.0 or above Upgrade FortiTokenMobile for iOS to version 5.3.0 or above Upgrade FortiTokenMobile for Windows to version 4.1.0 or above\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "5"
			}
		]
	},
