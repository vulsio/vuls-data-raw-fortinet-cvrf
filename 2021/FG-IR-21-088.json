	"documentnotes": {
		"note": [
			{
				"text": "\n            An unsafe search path vulnerability  in FortiClient and FortiClient EMS may allow an attacker to perform a DLL Hijack attack on affected devices via a malicious OpenSSL engine library in the search path.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            None\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Execute unauthorized code or commands\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            FortiClient 7.0.0 FortiClient 6.4.6 and below. FortiClient 6.2 all versions FortiClient 6.0 all versions FortiClient EMS 7.0.0 FortiClient EMS 6.4.6 and below. FortiClient EMS 6.2 all versions FortiClient EMS 6.0  all versions\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "4"
			},
			{
				"text": "\n            Please upgrade to FortiClient 7.0.1 or above. Please upgrade to FortiClient 6.4.7 or above.   Please upgrade to FortiClient EMS 7.0.1 or above. Please upgrade to FortiClient EMS 6.4.7 or above.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "5"
			}
		]
	},
