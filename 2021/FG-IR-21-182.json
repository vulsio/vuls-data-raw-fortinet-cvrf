	"documentnotes": {
		"note": [
			{
				"text": "\n            A privilege escalation vulnerability in FortiNAC may allow an admin user to escalate the privileges to root via the sudo command.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            None\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Escalation of privilege\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            FortiNAC version 8.8.8 and below. FortiNAC version 9.1.2 and below.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "4"
			},
			{
				"text": "\n            Please upgrade to FortiNAC version 8.8.9 or above. Please upgrade to FortiNAC version 9.1.3 or above. Please upgrade to FortiNAC version 9.2.0 or above.    \n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "5"
			}
		]
	},
