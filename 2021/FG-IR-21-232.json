	"documentnotes": {
		"note": [
			{
				"text": "\n            An incorrect permission assignment for critical resource vulnerability [CWE-732] in FortiClient for Linux may allow an unauthenticated attacker to access sensitive information in log files and directories via symbolic links.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            None\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Information disclosure\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            FortiClientLinux version 6.0.0 through 6.0.8FortiClientLinux version 6.2.0 through 6.2.9FortiClientLinux version 6.4.0 through 6.4.7FortiClientLinux version 7.0.0 through 7.0.2\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "4"
			},
			{
				"text": "\n            Please upgrade to FortiClientLinux version 7.0.3Please upgrade to FortiClientLinux version 6.4.8\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "5"
			}
		]
	},
