	"documentnotes": {
		"note": [
			{
				"text": "\n            An improper neutralization of special elements used in an OS command vulnerability [CWE-78] in the command line interpreter of FortiAuthenticator may allow an authenticated attacker to execute unauthorized commands via specifically crafted arguments to existing commands.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            None\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Execute unauthorized code or commands\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            FortiAuthenticator 6.3.0 and below.FortiAuthenticator 6.2.1 and below.FortiAuthenticator 6.2.0 and below.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "4"
			},
			{
				"text": "\n            Please upgrade to FortiAuthenticator version 6.4.0 or abovePlease upgrade to FortiAuthenticator version 6.3.1 or above\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "5"
			}
		]
	},
