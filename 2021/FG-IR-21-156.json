	"documentnotes": {
		"note": [
			{
				"text": "\n            Multiple relative path traversal vulnerabilities [CWE-23] in the API of FortiWeb may allow an authenticated attacker to retrieve arbitrary files from the underlying filesystem via specially crafted web requests.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            None\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Improper access control\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            FortiWeb versions 6.4.1 and below. FortiWeb versions 6.3.15 and below. FortiWeb versions 6.2.6 and below. FortiWeb versions 6.1.2 and below. FortiWeb versions 6.3.15 and below.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "4"
			},
			{
				"text": "\n            Upgrade to FortiWeb 7.0.0 or above. Upgrade to FortiWeb 6.4.2 or above. Upgrade to FortiWeb 6.3.16 or above.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "5"
			}
		]
	},
