	"documentnotes": {
		"note": [
			{
				"text": "\n            An exposure of sensitive information to an unauthorized actor [CWE-200] vulnerability in FortiManager may allow a FortiGate user to see scripts from other ADOMS.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            None\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Information disclosure\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            FortiManager version 7.0.1 and below. FortiManager version 6.4.6 and below. FortiManager version 6.2.x. FortiManager version 6.0.x. FortiManager version 5.6.x.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "4"
			},
			{
				"text": "\n            Please upgrade to FortiManager version 7.0.2 or above. Please upgrade to FortiManager version 6.4.7 or above.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "5"
			}
		]
	},
