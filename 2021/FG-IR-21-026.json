	"documentnotes": {
		"note": [
			{
				"text": "\n            A missing release of memory after effective lifetime vulnerability in FortiSwitch may allow an attacker on an adjacent network to exhaust available memory by sending specifically crafted LLDP/CDP/EDP packets to the device.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Denial of service\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            FortiSwitch versions 6.4.6 and below.FortiSwitch versions 6.2.6 and below.FortiSwitch versions 6.0.xFortiSwitch versions 3.6.x\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Please upgrade to FortiSwitch 7.0.0.Please upgrade to FortiSwitch version 6.4.7 or above.Please upgrade to FortiSwitch version 6.2.7 or above.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
