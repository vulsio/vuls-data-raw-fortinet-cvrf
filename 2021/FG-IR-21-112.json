	"documentnotes": {
		"note": [
			{
				"text": "\n            An information disclosure vulnerability [CWE-200] in FortiAnalyzer and FortiManager VM may allow an authenticated attacker to read the FortiCloud credentials which were used to activate the trial license in cleartext.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            None\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Information Disclosure\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            FortiManager version 7.0.0. FortiManager versions 6.4.6 and below.   FortiAnalyzer version 7.0.0. FortiAnalyzer versions 6.4.6 and below.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "4"
			},
			{
				"text": "\n            Please upgrade to FortiManager version 6.4.7 or above. Please upgrade to FortiManager version 7.0.1 or above.   Please upgrade to FortiAnalyzer version 6.4.7 or above. Please upgrade to FortiAnalyzer version 7.0.1 or above.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "5"
			}
		]
	},
