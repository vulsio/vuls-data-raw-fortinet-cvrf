	"documentnotes": {
		"note": [
			{
				"text": "\n            Multiple improper neutralization of special elements used in an SQL command vulnerabilities (CWE-89) in FortiPortal may allow an attacker with regular user's privileges to execute arbitrary commands on the underlying SQL database via specifically crafted HTTP requests.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Arbitrary code execution, information disclosure\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            FortiPortal 6.0.4 and below.  FortiPortal 5.3.5 and below. FortiPortal 5.2.5 and below. FortiPortal 5.1.2 and below. FortiPortal 5.0.3 and below. FortiPortal 4.2.4 and below. FortiPortal 4.1.2 and below. FortiPortal 4.0.4 and below.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Please upgrade to FortiPortal version 6.0.5 or above Please upgrade to FortiPortal version 5.3.6 or above Please upgrade to FortiPortal version 5.2.6 or above\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
