	"documentnotes": {
		"note": [
			{
				"text": "\n            An improper neutralization of input during web page generation vulnerability [CWE-79] in FortiProxy and FortiOS web filter override form may allow an unauthenticated attacker to perform an XSS attack via crafted HTTP GET requests.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            None\n        ",
				"title": "Description",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            Information disclosure\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            FortiOS version 7.0.3 and below,FortiOS version 6.4.8 and below,FortiOS version 6.2.10 and below,FortiOS version 6.0.14 to 6.0.0.FortiProxy version 7.0.1 and below,FortiProxy version 2.0.7 to 2.0.0.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "4"
			},
			{
				"text": "\n            Please upgrade to FortiOS version 7.0.4 or above Please upgrade to FortiOS version 6.4.9 or above Please upgrade to FortiOS version 6.2.11 or above Please upgrade to FortiProxy version 7.0.2 or above Please upgrade to FortiProxy version 2.0.8 or above\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "5"
			}
		]
	},
