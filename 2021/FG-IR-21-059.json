	"documentnotes": {
		"note": [
			{
				"text": "\n            An improper access control vulnerability in FortiManager and FortiAnalyzer GUI interface may allow a remote and authenticated attacker with restricted user profile toÂ retrieve the list of administrative users of other ADOMs andÂ their related configuration.\n        ",
				"title": "Summary",
				"type": "Summary",
				"ordinal": "1"
			},
			{
				"text": "\n            Improper Access Control\n        ",
				"title": "Impact",
				"type": "General",
				"ordinal": "2"
			},
			{
				"text": "\n            FortiManager version 7.0.0. FortiManager version 6.4.5 and below. FortiManager version 6.2.8 and below. FortiManager version 6.2.x. FortiManager version 6.0.x. FortiManager version 5.6.x. FortiAnalyzer version 7.0.0. FortiAnalyzer version 6.4.5 and below. FortiAnalyzer version 6.2.8 and below. FortiAnalyzer version 6.0.x. FortiAnalyzer version 5.6.x.\n        ",
				"title": "Affected Products",
				"type": "General",
				"ordinal": "3"
			},
			{
				"text": "\n            Please upgrade to FortiManager version 7.0.1 or above. Please upgrade to FortiManager version 6.4.6 or above. Please upgrade to FortiManager version 6.2.9 or above. Please upgrade to FortiAnalyzer version 7.0.1 or above. Please upgrade to FortiAnalyzer version 6.4.6 or above. Please upgrade to FortiAnalyzer version 6.2.9 or above.\n        ",
				"title": "Solutions",
				"type": "General",
				"ordinal": "4"
			}
		]
	},
